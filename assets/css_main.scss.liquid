  #site-cart-handle.hover & .count {
    animation-name: scroll-in;
  }
  #site-cart-handle.hover-out & .count {
    animation-name: scroll-out;
  }

  .count-holder {
    position: absolute;
    top: 8px;
    left: 0;
    width: 24px;
    text-align: center;
    font-size: rem(13px);
    line-height: 1;
    @include font-bold;
