#add-to-cart {
  .product-variant {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 30px;

    @include at-query($min, $portable+1) {
      justify-content: flex-start;
    }
  }

  label {
    font-size: 13px;
    @include font-thin;
    text-transform: uppercase;
    &:after {
      content: ":";
    }
  }

  &.style--classic {
    label {
      display: block;
    }
    
    .product-variant {
      display: inline-block;
      &:not(:first-child) {
        margin-left: 30px;
      }
    }

    .cart-functions {
      margin-top: 65px;
    }
  }

  &.style--minimal {
    ul {
      display: flex;
      border: 1px solid $blueColor;

      @include at-query($min, $palm+1) {
        margin: 0 0 0 33px;
      }
    }

    li {
      display: flex;
      align-items: center;
      position: relative;
      min-width: 85px;
      margin: 0;
      cursor: pointer;
      text-align: center;
      background-color: $whiteColor;
      transition: background-color 0.2s;

      &:not(:last-child) {
        border-right: 1px solid $blueColor;
      }

      &.active,
      &:hover {
        background-color: $blueColor;

        span { color: $whiteColor; }
      }

      &.disabled {
        pointer-events: none;

        span {
          opacity: .12;
          text-decoration: line-through;
        }
      }

      span {
        display: block;
        width: 100%;
        padding: 8px 20px;
        @include font-thin;
        font-size: 13px;
        line-height: 15px;
        transition: color 0.2s;
      }

      &:focus {
        outline: none !important;
      }
    }

    ul.color-true li {
      text-indent: -9999px;
      opacity: 1;
      top: -11px;

      span {
        width: 100%;
        height: 100%;
        margin-top: 0;
        font-size: 0;
      }

      &:after {
        display: none;
      }
    }
  }
}

