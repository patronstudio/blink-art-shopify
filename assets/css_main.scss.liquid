  top: 0;
  right: 0;
  width: 1px;
  height: 100%;
  background: $bodyBg;
  z-index: 9; 
  //content: "";
}

body {
  max-width: 1600px;
  margin: 0 auto;
  font-size: rem(15px);
  @include font-regular;
  line-height: 1.7;
  color: $textMain;
  background: $bodyBg;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  position: relative;
  right: 0;
  overflow-x: hidden;
