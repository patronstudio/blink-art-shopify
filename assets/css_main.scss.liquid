    p { margin: 0; }
  }
  table tr {
    border-top: 1px solid $bodyBgThird;
  }
  table thead tr {
    border-top: none;
  }
  table th,
  table td {
    min-height: 41px;
    padding: 10px 14px 8px;
    border-right: 1px solid $bodyBgThird;
