    font-size: rem(32px);
    margin-bottom: 40px;
  }
  h2 {
    font-size: rem(24px);
    margin-bottom: 34px;
  }
  h3 {
    font-size: rem(20px);
    margin-bottom: 30px;
  }
  h4 {
    font-size: rem(17px);
    margin-bottom: 27px;
  }
  h5,
  h6 {
    font-size: rem(15px);
    margin-bottom: 15px;
    opacity: .66;
    @include font-regular;
  }

  blockquote {
    font-family: $quoteFont;
    font-size: rem(24px);
    margin-bottom: 14px;
    font-style: italic;
    display: block;
  }

  ul,
  ol {
    padding-left: 17px;
    margin-bottom: 24px;
    li {
      margin-bottom: 12px;
    }
  }
  ul:not(.no-bullets) {
    list-style: disc;
    ul {
      list-style: circle;
      ul {
        list-style: square;
      }
    }
  }
  ol:not(.no-bullets) {
    list-style: decimal;
    ol {
      list-style: lower-roman;
      ol {
        list-style: lower-alpha;
      }
    }
  }

  img {
    width: 100%;
    margin-bottom: 24px;
  }

  &.dark {
