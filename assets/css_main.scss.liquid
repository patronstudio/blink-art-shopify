/* ----
   Tabs & toggles addon
  ---
*/

.krown-tabs {

  .titles {
    border-bottom: 3px solid $bodyBgThird;
    @extend .clearfix;
  }

  .titles h5 {

    display: block;
    margin: auto;
    font-size: 12px;
    line-height: 60px;
    @include font-regular;
    color: $textMain;
    text-transform: uppercase;
    line-height: 16px;
    padding: 2px 30px 0;
    float: left;
    height: 60px;
    line-height: 60px;
    cursor: pointer;
    opacity: 1;
    transition: all 150ms linear;

    &:hover {
      background: $bodyBgSecond;
    }
    &.opened {
      background: $bodyBgThird !important;
      cursor: default;
      pointer-events: none;
    }
  }

  .contents {

    min-height: 200px;
    padding: 50px 30px 0;

    >div {
      display: none;
      &.opened,
      &:first-child {
        display: block;
      }
    }
  }

  &[data-design="toggles"] {

    .titles {
      border: none;
    }

    .titles h5 {

      border: 3px solid $bodyBgThird;
      margin-bottom: 24px;
      line-height: 54px;
      float: none;
      width: 100%;
      position: relative;

      &:after {
        content: "+";
        position: absolute;
        right: 30px;
        font-size: 28px;
        @include font-regular;
      }
      &.opened:after {
        content: "-";
        margin-top: -2px;
      }

      &.opened {
        cursor: pointer;
        pointer-events: all;
      }

      &:last-child {
        margin-bottom: 0;
      }

      +div {
        padding: 6px 30px 12px;
        display: none;
      }
    }

    .contents {
      display: none;
    }
  }
}

