.site-footer {
  padding: 60px 40px 40px;
  font-size: rem(13px);
  @include font-bold;
  color: $footerTxt;
  border-top: 1px solid $bodyBgThird;
  z-index: 9;

  @include at-query($min, $lap+1) {
    display: flex;
    flex-wrap: wrap;
  }

  @include at-query($max, $lap) {
    padding: 40px !important;

    .site-box {
      padding: 0 !important;
    }

    .footer-content:not(:last-child) {
      margin-bottom: 40px !important;
    }

    .to-bottom-content {
      bottom: -20px !important;
    }
  }

  .footer-box {
    display: flex;
  }

  a {
    @include font-bold;
    color: $footerTxt;
    
    &:hover,
    &.hover {
      color: $bodyAcc;
    }
  }

  .footer-content,
  .site-copyright {
    width: 100%;

    @include at-query($min, $lap) {
      width: 50%;
    }

    .ie9 & {
      margin: auto;
    }
  }
  .footer-content:not(:last-child) {
    margin-bottom: 40px;
  }

  h4 {
    margin-bottom: 20px;
    font-size: 18px;
    text-align: right;

    @include at-query($max, $lap) {
      text-align: left;
    }
  }
  
  .caption {
    opacity: .78;
    margin-top: 25px;
  } 
  
  // widgets
  .info {
    font-size: rem(37px);
    line-height: 1;
    @include font-bold;

    @include at-query($max, $palm) {
      font-size: rem(27px);
    }
  }

  .footer-links {
    // @include at-query($min, $palm+1) {
    //   display: flex;
    //   flex-wrap: wrap;

    //   li {
    //     width: 50%;
    //   }
    // }

    li {
      width: 100%;

      &:not(:last-child) {
        margin-bottom: 10px;
      }
    }
  }

  .site-copyright {
    opacity: .66;
  }

  // .site-box {
  //   border-bottom: 0 !important;
  //   border-right: 1px solid lighten($textMain, 5%) !important;
  //   background: $textMain !important;
  // }

  .input-group {
    display: flex;
    justify-content: flex-end;

    @include at-query($max, $lap) {
      justify-content: flex-start;
    }
  }
  
  input:not([type="submit"]) {
    @include font-thin;
    width: 300px;
    background: $whiteColor !important;
    color: $bodyBg !important;
    border-width: 1px 0 1px 1px;
    border-style: solid;
    border-color: $greyColor;
    // @include input-placeholder() {
    //   color: rgba($bodyBg, .66) !important;
    // }
    // &:focus {
    //   background: darken($textMain, 3%) !important;
    // }
  }

  input[type="submit"] {
    top: 0;
    width: 75px;
    min-width: 75px;
    height: 40px;
    line-height: 40px;
    padding: 0;
    font-size: 18px;
    border: 0;
    transition: background-color 0.2s;

    &:hover {
      background-color: $textMain!important;
    }
  }

  @media all and (max-width: $portable) and (min-width: $lap),
  all and (max-width: $palm) {
    input[type="submit"] {
      width: auto;
    }
  }

  .errors {
    position: relative;
    top: -15px;
  }
}

.site-preloader {
  position: fixed;
  background: url({{ settings.preloader | img_url: '2048x2048' }}) no-repeat center center transparent;
  background-size: 75px 75px;
  width: 75px;
  height: 75px;
  top: 50%;
  left: 50%;
  transform: translate3d(-50%, -50%, 0);
  z-index: 99999;
  transition: opacity 150ms linear;
  pointer-events: none;

  &.out {
    opacity: 0;
  }
}

.site-sharing {

  position: fixed;
  height: auto;
  width: 26px;
  left: 60px;
  bottom: 60px;
  cursor: pointer;
  z-index: 99;
  padding-bottom: 30px;

  &.rss {
    padding-bottom: 0;
    a {
      opacity: 1;
      visibility: visible;
      margin: 0;
      transform: none;
    }
  }

  body.footer-in-viewport & {
    position: absolute;
  }

  .icon {
    display: block;
    width: 30px;
    height: 30px;
    position: absolute;
    bottom: 0;
  }

  svg * {
    transition: all 150ms linear;
    fill: $textMain;
  }
  &.white svg * {
    fill: $whiteColor;
  }

  a {

    width: 26px;
    height: 26px;
    margin-bottom: 25px;
    transform: translateY(50%);
    opacity: 0;
    visibility: hidden;
    display: block;

    &:nth-child(2) {
      transition: all 150ms linear;
    }
    &:nth-child(3) {
      transition: all 150ms linear 100ms;
    }
    &:nth-child(4) {
      transition: all 150ms linear 200ms;
    }
    &:nth-child(5) {
      transition: all 150ms linear 300ms;
    }

    &:hover svg *,
    &.hover svg * {
      fill: $bodyAcc;
    }
  }

  .no-touchevents &:hover a,
  .touchevents &.hover a {
    transform: translateY(0);
    opacity: 1;
    visibility: visible;
  }

  @include at-query($max, $lap) {
    display: none;
  }

  @include sidebar-transition(transform);
  body.sidebar-move & {
    transform: translateX(-480px);
    @include at-query($max, '640px') {
      transform: translateX(-320px);
    }
  }
}

.site-social {
  a {
    display: inline-block;
    position: relative;
    width: 36px;
    height: 36px;
    margin: 0 7px;
    border-radius: 50%;
    background-color: $bodyAcc;
    transition: background-color 0.2s;

    &:hover {
      background-color: $textMain;
    }

    .site-nav & {
      opacity: .62;
      &:hover,
      &.hover {
        opacity: 1;
      }
    }
  }

  svg {
    position: absolute;
    top: 50%; left: 50%;
    transform: translate(-50%, -50%);
    width: 20px !important;
    height: 20px !important;

    path {
      fill: $whiteColor;
      transition: fill 150ms linear;
    }
  }

  .icon {}
}

.site-payment {
  .site-footer & {
    svg * {
      fill: $bodyBg;
    }
  }
}


