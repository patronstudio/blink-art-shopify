.box__product-gallery {
  &.animate {
    transition: all 300ms $easeInOutSine;
  }
  &.expand {
    width: 100%;
  }

  &.scroll .site-box-content {
    @include at-query($max, $portable) {
      &:after {
        content: 'flickity';
        display: none;
      }
      height: 100vh;
    }
  }

  @include at-query($min, $lap+1) {
    border-bottom: none !important;
  }

  &.expand .gallery-item {
    height: 0 !important;
  }

  &.slider .site-box-content {
    height: 100vh;
  }

  @include at-query($max, $lap) {
    min-height: 0 !important;
    height: 70vh !important; //padding-top: 83px; // header height here
    overflow: visible;
    .site-box-content {
      height: 100% !important;
    }
  }

  @media screen and (max-width: $lap) and (orientation: landscape) {
    height: 164vh !important;
  }

  &:after {

    content: "";
    display: block;
    position: fixed;
    width: 50%;
    height: 170px;
    bottom: 0;
    left: 0;
    z-index: 9;

    background: -moz-linear-gradient(top, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0.3) 100%);
    background: -webkit-linear-gradient(top, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0.3) 100%);
    background: linear-gradient(to bottom, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0.3) 100%);

    @include sidebar-transition(left);
    body.sidebar-move & {
      left: -480px;
      @include at-query($max, '640px') {
        left: -320px;
      }
    }

    @include at-query($max, $lap) {
      display: none;
    }
  }
}

