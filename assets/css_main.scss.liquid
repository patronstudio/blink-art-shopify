    &.is-selected {
      opacity: .66;
      background-color: $textMain;
    }
  }
}

.slider-nav svg * {
  fill: $textMain;
}

.icon-go {
  position: absolute;
  overflow: hidden;
  cursor: pointer;
  top: 1px;
  width: 42px;
  height: 20px;
  transition: all 150ms linear;

  svg {
    margin-left: -25px;
    margin-top: 5px;
    display: block;
    transition: all 150ms linear;
    * {
      fill: $textMain;
    }
  }

  &:hover svg,
  &.hover svg {
    margin-left: 0;
  }

  &.go-next {
    text-align: right;
    right: -38px;
    .gallery-index & {
      right: -58px;
    }
  }
  &.go-prev {
    text-align: left;
    left: -38px;
    transform: rotate(180deg);
    .gallery-index & {
      left: -58px;
    }
  }
  &.go-down {
    transform: rotate(90deg);
    top: 100%;
    left: 50%;
    right: auto;
    margin-left: -20px;
    margin-top: 5px;
    z-index: 999;
    opacity: .66;
    svg {
      margin-left: -35px;
    }
  }
}

.site-nav li.has-submenu a:hover .icon-go,
.site-nav li.has-submenu a.hover .icon-go {
  svg {
    margin-left: 0;
    * {
      fill: $bodyAcc;
    }
  }
}

.site-nav li.has-submenu:hover .icon-go.go-down,
.site-nav li.has-submenu.hover .icon-go.go-down {
  margin-top: 15px;
}

.site-nav li.has-submenu a.active+.icon-go svg * {
  fill: $bodyAcc;
}

.product-zoom {
  position: fixed;
  right: 50%;
  top: 0;
  width: 36px;
  height: 36px;
  overflow: visible;
  z-index: 666;
  margin-right: 54px;
  cursor: pointer;
  margin-left: -18px;

  top: auto;
  bottom: 58px;
  right: auto;
  left: 25%;

  &.no-sharing {
    left: 70px;
  }

  .no-touchevents &:hover,
  .no-touchevents &.hover {
    polyline:nth-child(1) {
      transform: scale(0.9) translate3d(-2px, -2px, 0);
    }
    polyline:nth-child(2) {
      transform: scale(0.9) translate3d(2px, -2px, 0);
    }
    polyline:nth-child(3) {
      transform: scale(0.9) translate3d(2px, 2px, 0);
    }
    polyline:nth-child(4) {
      transform: scale(0.9) translate3d(-2px, 2px, 0);
    }
  }

  polyline {
    transform: scale(0.9);
    transform-origin: center;
    transition: transform 150ms linear;
    fill: $textMain;
  }

  &.expand {
    top: 40px; left: initial; right: 0;
  }

  span {
    position: absolute;
  }

